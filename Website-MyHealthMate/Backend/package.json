{
  "name": "diabetes-prediction-backend",
  "version": "1.0.0",
  "description": "Backend for Diabetes Prediction Website",
  "main": "dist/server.js",
  "scripts": {
    "lint": "eslint src --ext js --report-unused-disable-directives --max-warnings 0",
    "clean-dist": "node -e \"const fs = require('fs'); if (fs.existsSync('dist')) fs.rmSync('dist', {recursive: true}); fs.mkdirSync('dist');\"",
    "build-babel": "babel ./src -d ./dist --copy-files",
    "build": "npm run clean-dist && npm run build-babel",
    "production": "npm run build && cross-env NODE_ENV=production node ./dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/server.js",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "diabetes",
    "prediction",
    "health",
    "mongodb"
  ],
  "author": "MyHealthMate Team",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "axios": "^1.13.1",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^5.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-status-codes": "^2.3.0",
    "joi": "^17.11.0",
    "mongodb": "^6.9.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/node": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^9.36.0",
    "nodemon": "^3.0.1"
  }
}
